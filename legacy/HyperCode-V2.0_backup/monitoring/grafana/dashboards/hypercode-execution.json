{
  "title": "HyperCode Execution",
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "HTTP 5xx Rate",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{status=~\"5..\"}[5m])",
          "legendFormat": "5xx"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Request Rate",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count[5m])",
          "legendFormat": "req/s"
        }
      ]
    }
    ,
    {
      "type": "graph",
      "title": "Interpreter Executions by Status",
      "targets": [
        {
          "expr": "sum(rate(interpreter_executions_total[5m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Interpreter Errors by Type",
      "targets": [
        {
          "expr": "sum(rate(interpreter_errors_total[5m])) by (error_type)",
          "legendFormat": "{{error_type}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Interpreter Exec Avg Duration (s)",
      "targets": [
        {
          "expr": "rate(interpreter_execute_duration_seconds_sum[5m]) / rate(interpreter_execute_duration_seconds_count[5m])",
          "legendFormat": "avg"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Interpreter Exec Duration p95 (s)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(interpreter_execute_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    }
  ]
}
